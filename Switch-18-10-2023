       var tinta='amarela';
       switch(tinta){
        Case'amarela','amarelo','Amarelo':
        document.write("Irei pintar o carro de amarelo");
        break;
        Case'azul':
        document.write("Irei pintar de azul");
        break;
        default:
            document.write('nao irei pintar')
       }
    </script>
